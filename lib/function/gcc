    echo ARM64_CROSS='"'CC=/usr/bin/aarch64-linux-gnu-gcc-10'"' >> gcc.txt
    source gcc.txt
    ;;
esac
case `grep -Fx 'DISTRO_VERSION="hirsute"' "userdata.txt" >/dev/null; echo $?` in
  0)
    echo NATIVE='"'CC=/usr/bin/gcc-10'"' > gcc.txt
    echo ARM64_CROSS='"'CC=/usr/bin/aarch64-linux-gnu-gcc-10'"' >> gcc.txt
    source gcc.txt
    ;;
esac
case `grep -Fx 'DISTRO_VERSION="impish"' "userdata.txt" >/dev/null; echo $?` in
  0)
    echo NATIVE='"'CC=/usr/bin/gcc-10'"' > gcc.txt
    echo ARM64_CROSS='"'CC=/usr/bin/aarch64-linux-gnu-gcc-10'"' >> gcc.txt
    source gcc.txt
